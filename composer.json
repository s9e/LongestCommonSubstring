{
	"name": "s9e/longest-common-substring",
	"type": "library",
	"description": "Longest common substring generator",
	"homepage": "https://github.com/s9e/LongestCommonSubstring/",
	"keywords": ["lcs"],
	"license": "MIT",
	"require": {
		"php": ">=8.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.0",
		"s9e/repdoc": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"s9e\\LongestCommonSubstring\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"s9e\\LongestCommonSubstring\\Tests\\": "tests"
		}
	},
	"scripts": {
		"patch-docs": "vendor/bin/repdoc README.md",
		"test": "vendor/bin/phpunit"
	}
}
